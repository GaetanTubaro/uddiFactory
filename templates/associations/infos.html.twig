{% extends 'base.html.twig' %}

{% form_theme form 'bootstrap_5_horizontal_layout.html.twig' %}

{% block title %}
	Modifier mes informations
{% endblock %}

{% block body %}
	<div class="w-50 mx-auto mt-5">
		{{ form_start(form) }}
		{{ form_row(form.username) }}
		{{ form_row(form.plainPassword) }}
		{{ form_row(form.name) }}
		{{ form_row(form.valider) }}
		{{ form_end(form) }}
	</div>
{% endblock %}
